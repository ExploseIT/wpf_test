<UserControl x:Name="uc_FileConcatenate" x:Class="wpf_test.UCs.ucFileConcatenate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:wpf_test.UCs"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             d:Background="White">
    <StackPanel x:Name="uc_Sqlite" VerticalAlignment="Top">
        <Label Content="File Concatenate" HorizontalAlignment="Center" Height="30"  VerticalAlignment="Top" />
        <Image Name="imgOutput" Width="300" Height="300"/>
        <Label Name="labHashValue" HorizontalAlignment="Center" Width="300" Content=""/>
        <Button Content="Exit" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,10,0,0" Click="OnExit_Click"/>


    </StackPanel>
</UserControl>
